{
  "name": "czmask",
  "version": "1.1.0",
  "description": "CzMask, mask for inputs",
  "main": "src/index.js",
  "scripts": {
    "serve": "rm -rf ./serve && rm -rf ./.cache && parcel src/index.html --out-dir serve",
    "build": "parcel build src/index.html --out-dir build --public-url ./",
    "make": "parcel build src/CzMask.js",
    "clean": "rm ./serve && rm -rf ./.cache && rm -rf ./build && rm -rf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cellez/CzMask.git"
  },
  "keywords": [
    "cellez",
    "CzMask",
    "mask",
    "html",
    "input"
  ],
  "author": "JonellRz",
  "license": "MIT",
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  }
}
